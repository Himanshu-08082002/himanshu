
#include <stdio.h>
#include<math.h>
int main()
{
    int a,b,c,D,r1,r2;
    printf("Enter the value of a,b,c\n");
    scanf("%d%d%d",&a,&b,&c);
    D=(b*b)-(4*a*c);
    if(D>0){
    r1=(-b+sqrt(D))/(2*a);
    r2=(-b-sqrt(D))/(2*a);
    printf("roots are:\n%d\n%d",r1,r2);
    }
    else if(D==0){
    r1=(-b+sqrt(D))/(2*a);
    printf("roots are equal:\n%d",r1);
    }
    else if(D<0){
        printf("Imaginary Roots");
    }
    return 0;
}
